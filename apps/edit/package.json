{
  "name": "@ecmaos-apps/edit",
  "version": "0.1.1",
  "type": "module",
  "bin": {
    "edit": "./dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc && node build.cjs",
    "dev": "tsc --watch",
    "publish:dev": "npm run build && npm publish --registry=http://localhost:4873",
    "republish:dev": "npm run unpublish:dev && npm run publish:dev",
    "unpublish:dev": "npm unpublish @ecmaos-apps/edit --registry=http://localhost:4873 --force"
  },
  "dependencies": {
    "ansi-escape-sequences": "^6.2.1"
  },
  "devDependencies": {
    "@ecmaos/config-typescript": "workspace:^",
    "@ecmaos/types": "workspace:^",
    "@types/ansi-escape-sequences": "^4.0.4",
    "esbuild": "^0.27.2",
    "esbuild-plugins-node-modules-polyfill": "^1.7.1",
    "typescript": "^5.6.3"
  }
}
