{
  "name": "@ecmaos-apps/web3",
  "version": "0.1.2",
  "type": "module",
  "bin": {
    "web3": "./dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc && node build.cjs",
    "dev": "tsc --watch",
    "publish:dev": "npm run build && npm publish --registry=http://localhost:4873",
    "republish:dev": "npm run unpublish:dev && npm run publish:dev",
    "unpublish:dev": "npm unpublish @ecmaos-apps/web3 --registry=http://localhost:4873 --force"
  },
  "devDependencies": {
    "@ecmaos/config-typescript": "workspace:^",
    "@ecmaos/types": "workspace:^",
    "esbuild": "^0.27.2",
    "typescript": "^5.6.3",
    "viem": "^2.45.1"
  }
}
